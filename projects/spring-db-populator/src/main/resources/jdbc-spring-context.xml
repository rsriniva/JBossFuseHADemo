<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="dataSource" class="org.springframework.jdbc.datasource.DriverManagerDataSource">
 
		<property name="driverClassName" value="com.redhat.poc.jdbc.AmqJdbcDriver" />
		<property name="url" value="jdbc:amqjdbc://localhost/jdbcpoc" />
		<property name="username" value="postgres" />
		<property name="password" value="12345678"/>
	</bean>
	
	<bean id="jdbcPocDao" class="com.redhat.poc.jdbc.JdbcPocDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<!-- bean id="gpsDao" class="mx.org.fimpe.sati.transporte.comun.gps.dao.GpsDAO">
		<property name="dataSource" ref="dataSource" />
	</bean -->

</beans>
